% --- Executes on button press in pushbutton1.
function pushbutton1_Callback(hObject, eventdata, handles)
% hObject    handle to pushbutton1 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
D12=str2num(get(handles.A,'string'))
D23=str2num(get(handles.B,'string'))
D13=str2num(get(handles.C,'string'))
 
contents=cellstr(get(handles.popupmenu2,'string'));
popChoice=contents(get(handles.popupmenu2,'value'));
assignin('base','popChoice',popChoice);
if(strcmp(popChoice,'Drake'));
    popVal=1;
    rL=0.01143;
    rC=0.014;
elseif(strcmp(popChoice,'Warving'))
    popVal=2;
    rL=0.3831;
    rC=0.609;
elseif(strcmp(popChoice,'Dove'));
    popVal=3;
    rL=0.0314;  
    rC=0.927;
elseif(strcmp(popChoice,'OSTRICH'));
    popVal=4;
    rL=0.00697;
    rC=0.008636;
elseif(strcmp(popChoice,'Warving'));
    popVal=5;
    rL=0.01417;
    rC=0.01762;
end
assignin('base','popVal',popVal);
 
Dm= power((D12*D23*D13),1/3);
L= 2*10^-7*log(Dm/rL);
set(handles.G,'string', num2str(L))
k = 8.85*10^-12;
C= (2*pi*k)/(log(Dm/rC));
set(handles.H,'string', num2str(C))
 
 
% --- Executes on button press in pushbutton2.
function pushbutton2_Callback(hObject, eventdata, handles)
% hObject    handle to pushbutton2 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
set(handles.A, 'string','');
set(handles.B, 'string','');
set(handles.C, 'string','');
set(handles.G, 'string','');
set(handles.H, 'string','');
 
 
% --- Executes on button press in pushbutton3.
function pushbutton3_Callback(hObject, eventdata, handles)
% hObject    handle to pushbutton3 (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA
 
 
e = str2num(get(handles.M,'string'))
l = str2num(get(handles.N,'string'))
b = str2num(get(handles.O,'string'))
W = str2num(get(handles.P,'string'))
T = str2num(get(handles.Q,'string'))
temp = str2num(get(handles.R,'string'))
 
%% Maximum Power Transmitted in KVA
pkva=150*(((e.^2)/30.25)-(l/1.6))
set(handles.S,'string', num2str(pkva))
 
%% calculating ground clearance
 GC = 5.2 + (0.3)*((e-33)/33); 
 set(handles.T,'string', num2str(GC))
 
%% Sag calculation....100 is the span length
sag = (W*50*50)/(8*T);
set(handles.U,'string', num2str(sag))
 
 
%% Spacing between conductors
sb_cond = (5.08+1.814*e + (50^2)/(772.84))/100;
 
%% Critical Disruptive voltage calcuation
    
    %% air density factor calculation
    ADF = (3.92*b)/(273+temp);
    set(handles.V,'string', num2str(ADF))
    
    r = (0.5*22.6);
    Vc = 21.2*ADF*0.85*r*100*log(sb_cond/r); 
    Z = (244/ADF)*((50+25)/(((e/sqrt(3))-Vc)^2))*sqrt(r/sb_cond)*0.000001;
    set(handles.W,'string', num2str(Z))
 
 
 
 
% --- Executes on selection change in popupmenu1.
function popupmenu1_Callback(hObject, eventdata, handles)
 
 
 
